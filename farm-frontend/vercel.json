{
  "buildCommand": "npm run vercel-build",
  "functions": {
    "src/app/api/photos/[id]/route.ts": {
      "maxDuration": 30
    }
  },
  "redirects": [
    {
      "source": "https://farmcompanion.uk/:path*",
      "destination": "https://www.farmcompanion.co.uk/:path*",
      "permanent": true
    },
    {
      "source": "https://www.farmcompanion.uk/:path*",
      "destination": "https://www.farmcompanion.co.uk/:path*",
      "permanent": true
    },
    {
      "source": "https://farmshopfinder.co.uk/:path*",
      "destination": "https://www.farmcompanion.co.uk/:path*",
      "permanent": true
    },
    {
      "source": "https://www.farmshopfinder.co.uk/:path*",
      "destination": "https://www.farmcompanion.co.uk/:path*",
      "permanent": true
    },
    {
      "source": "https://localfarmshops.co.uk/:path*",
      "destination": "https://www.farmcompanion.co.uk/:path*",
      "permanent": true
    },
    {
      "source": "https://www.localfarmshops.co.uk/:path*",
      "destination": "https://www.farmcompanion.co.uk/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/api/photos/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
