{
  "name": "farm-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gen:og": "node tools/gen-og.mjs",
    "validate-images": "node scripts/validate-images.js",
    "check-images": "npm run validate-images",
    "lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:smoke": "playwright test tests/api-photos.spec.ts",
    "health-check": "node scripts/synthetic-health-check.js",
    "prod-health": "node scripts/production-health-check.js",
    "test:a11y": "playwright test --config=tests/a11y.config.ts",
    "test:performance": "lighthouse http://localhost:3000 --only-categories=performance --output=json",
    "test:accessibility": "lighthouse http://localhost:3000 --only-categories=accessibility --output=json",
    "test:map-qa": "node scripts/test-map-qa.js",
    "qa:map": "npm run test:map-qa",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "ANALYZE=true npm run build && npm run start",
    "vercel-build": "pnpm install && next build --no-lint"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.10",
    "@googlemaps/markerclusterer": "^2.6.2",
    "@react-email/components": "^0.5.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-virtual": "^3.13.12",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^0.25.1",
    "@vercel/kv": "^1.0.0",
    "@vis.gl/react-google-maps": "^1.5.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "form-data": "^4.0.4",
    "isomorphic-dompurify": "^2.12.0",
    "lucide-react": "^0.540.0",
    "next": "15.5.2",
    "prettier": "^3.6.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-virtuoso": "^4.14.0",
    "redis": "^5.8.2",
    "resend": "^6.0.1",
    "sharp": "^0.34.3",
    "tailwind-merge": "^2.2.1",
    "uuid": "^11.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.0",
    "@playwright/test": "^1.55.0",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "lighthouse": "^12.8.1",
    "playwright": "^1.42.1",
    "postcss": "^8.5.6",
    "puppeteer": "^24.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  },
  "browserslist": [
    "defaults and supports es6-module",
    "maintained node versions",
    "not ie <= 11",
    "not safari <= 12"
  ]
}
